package encapsulation;

public class Employee {

	//private variables
	private String name="kiruba";
	private int age=33;
	private double salary=23456.00;

	//getters and setters
	
	public String getName() {
		return name;
	}
	public int getAge() {
		return age;
	}
	public double getSalary() {
		return salary;
	}
}
